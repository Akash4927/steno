{
  "name": "steno",
  "version": "1.0.0",
  "description": "developer tool for integration testing of Slack Apps",
  "repository": "https://github.com/slackapi/steno.git",
  "bin": {
    "steno": "bin/steno"
  },
  "scripts": {
    "test": "npm run lint && npm run build && echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p tsconfig.json",
    "postbuild": "preprocess -f build/analytics.js -d build/analytics.js -t js",
    "clean": "rimraf build",
    "watch": "tsc -w",
    "lint": "tslint --project . src/**/*.ts",
    "start": "node bin/cli.js",
    "pkg": "npm run clean && npm run lint && npm run build && pkg --out-path pkg ."
  },
  "author": "Ankur Oberoi <aoberoi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^8.1.2",
    "npm": "^5.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.0",
    "@types/insight": "^0.8.0",
    "@types/lodash.clonedeep": "^4.5.3",
    "@types/lodash.isundefined": "^3.0.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^8.0.28",
    "@types/normalize-url": "^1.9.1",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^10.0.0",
    "pkg": "^4.2.6",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "^2.6.8",
    "express": "^4.15.4",
    "insight": "^0.8.4",
    "js-string-escape": "^1.0.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isundefined": "^3.0.1",
    "mkdirp": "^0.5.1",
    "normalize-port": "^1.0.0",
    "normalize-url": "^2.0.1",
    "pjson": "^1.0.9",
    "preprocess-cli-tool": "^1.0.1",
    "raw-body": "^2.3.2",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "analytics": {
    "googleTrackingId": "UA-56978219-12"
  }
}
