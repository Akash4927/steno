{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths":{
      "*": [
        "node_modules/*",
        "src/types/*"
      ]
    }
  },
  "include":[
    "src/**/*"
  ]
}